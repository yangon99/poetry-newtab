{
  "name": "poetry-newtab-extension",
  "version": "1.0.0",
  "description": "浏览器插件：替换新标签页，展示一句诗词",
  "main": "newtab.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "build:chrome": "npm run build && node scripts/build-chrome.js",
    "build:firefox": "npm run build && node scripts/build-firefox.js",
    "build:all": "npm run build && node scripts/build-all.js",
    "clean": "rimraf dist build",
    "check": "node scripts/check-build.js"
  },
  "keywords": [
    "browser-extension",
    "chrome-extension",
    "firefox-extension",
    "poetry",
    "newtab"
  ],
  "author": "yangon99",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "archiver": "^5.3.0",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "rimraf": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "jinrishici": "^1.0.6"
  }
}
